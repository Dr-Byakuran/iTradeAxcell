@model IEnumerable<iTrade.Models.StockTransaction>

    <!-- begin row -->
<div class="row">
    <!-- begin col-12 -->
    <div class="col-md-12">
        <!-- begin panel -->
        <div class="panel panel-inverse">

            <div class="panel-body">
                <div class="table-responsive">
                    <table id="data-table@{ @Html.ViewBag.TableNo }" class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th>
                                    NO
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CreatedOn)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ProcessType)
                                </th>

                                <th>
                                    @Html.DisplayNameFor(model => model.Qty)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.SourceFrom)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.RefNo)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Remark)
                                </th>
                            </tr>
                        </thead>
                        <tbody>

                                @{ int i = 0; }
                                @foreach (var item in Model)
                                {
                                    i++;
                                    <tr class="even gradeC">
                                        <td>
                                            @i
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.CreatedOn)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.ProcessType)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Qty)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.SourceFrom)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.RefNo)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.Remark)
                                        </td>


                                    </tr>
                                }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- end panel -->
    </div>
    <!-- end col-12 -->
</div>
<!-- end row -->
